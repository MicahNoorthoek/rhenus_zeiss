<div class="withdrawawlformheader">
  <h1 class="header-text">WithdrawalStatement<span id="tank_span">Tank: <%= @withdrawal_statement.pluck(:warehouse).first %></span></h1>
  <p class="under-header-text">Withdrawal Entry Number: <%= @withdrawal_statement.pluck(:withdrawalentrynumber).first %></p>
</div>
<div>

  <table class="data-table" cellpadding="0" cellspacing="0" style = "border:none;">
    <thead>
    <tr>
      <th class ="wms-table-header" width="20px;" style= "text-align:left; border:none !important; font-size: 14.28px;">WD #</th>
      <th class ="wms-table-header" width="100px;" style= "text-align:left; border:none !important; font-size: 14.28px;">Receipt Entry Number</th>
      <th class ="wms-table-header" width="100px;" style= "text-align:left; border:none !important; font-size: 14.28px;">Cust Ref Num</th>
      <th class ="wms-table-header" width="100px;" style= "text-align:left; border:none !important; font-size: 14.28px;">KM Ref Num</th>
      <th class ="wms-table-header" width="100px;" style= "text-align:right; border:none !important; font-size: 14.28px;"> Beginning Balance</th>
      <th class ="wms-table-header" width="100px;" style= "text-align:right; border:none !important; font-size: 14.28px;">Withdrawal Quantity</th>
      <th class ="wms-table-header" width="100px;" style= "text-align:right; border:none !important; font-size: 14.28px;">Ending Balance</th>
    </tr>
    </thead>
  <tbody>

    <% @withdrawal_statement.each_with_index do |f, i| %>
      <tr class='text-style row-style'>

        <td width="20px;" style = "text-align:left; padding-right: 7px; border:none;"><%= f.withdrawalnumber %></td>
        <td width="100px;" style = "text-align:left; padding-right: 7px; border:none;"><%= f.receiptentrynumber %></td>
        <td width="100px;" style = "text-align:left; padding-right: 7px; border:none;"><%= f.outboundreferencenumber %></td>
        <td width="100px;" style = "text-align:left; padding-right: 7px; border:none;"><%= f.salesreferencenumber %></td>
        <td width="100px;" style = "text-align:right; padding-right: 7px; border:none;"><%= f.beginningbalance%>  <%= f.uom %></td>
        <td width="100px;" style = "text-align:right; padding-right: 7px; border:none;"><%= f.withdrawalquantity %>  <%= f.uom %></td>
        <td width="100px;" style = "text-align:right; border:none;"><%= f.endingbalance %>  <%= f.uom %></td>
      </tr>
      <% end %>

  </tbody>
  </table>

</div>
