<html lang="en">
  <style>


</style>
<body class="container-fluid" style="width:100vw !important;">
  <div class="content-squish-screens justify-content-center">

    <div class="card">
      <div class="card-header">
        Recon Status
      </div>
      <div class="card-body">
         <table class="table table-hover">
          <thead>
                  <tr>
                      <th>Count</th>
                      <th>Comments</th>
                      <th>Min Part</th>
                      <th>Max Part</th>
                      <th></th>
                  </tr>
              </thead>

              <tbody>
                 <% @reconciliation.each do |s| %>
                  <tr>
                      <td><%= s.count_of_part_number %></td>
                      <td><%= s.comments %></td>
                      <td><%= s.min_of_part_number %></td>
                      <td><%= s.max_of_part_number %></td>
                      <td><button class="btn btn-search">List of parts</button></td>
                  </tr>
              </tbody>
          </table>
      </div>
    </div>
  </div>

  
</body>
</html>
